  "dependencies": {
    "apollo-server-express": "^2.11.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "epilogue": "^0.7.1",
    "express": "^4.17.1",
    "graphql": "^14.6.0",
    "http": "0.0.1-security",
    "ncp": "^2.0.0",
